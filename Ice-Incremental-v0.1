local function teleportLoop()
	while teleportRunning do
		local folder = workspace:FindFirstChild("IceFolder")
		if folder then
			local children = folder:GetChildren()
			if #children > 0 then
				for i = 1, #children do
					if not teleportRunning then break end
					local part = children[i]
					if part and part:IsA("BasePart") and humanoidRootPart then
						humanoidRootPart.CFrame = part.CFrame + Vector3.new(0, 5, 0)
					end
					task.wait(0.1)
				end
			end
		end
		task.wait(1)
	end
end

local function buyLoop()
	local BuyEvent = game:GetService("ReplicatedStorage"):WaitForChild("BuyEvents"):WaitForChild("BuyIceUpgrade")
	local index = 1
	while buyRunning do
		local args = {index, "Single"}
		BuyEvent:FireServer(unpack(args))
		index = index + 1
		if index > 4 then index = 1 end
		task.wait(0.5)
	end
end

local function rebirthLoop()
	local RebirthEvent = game:GetService("ReplicatedStorage"):WaitForChild("Resets"):WaitForChild("Frost")
	while rebirthRunning do
		RebirthEvent:FireServer()
		task.wait(1)
	end
end

local function upgrdLoop()
	local UpgrdEvent = game:GetService("ReplicatedStorage"):WaitForChild("BuyEvents"):WaitForChild("BuyBlizzardUpgrade")
	local queue = {3, 2, 1}
	local index = 1
	while upgrdRunning do
		local args = {queue[index], "Single"}
		UpgrdEvent:FireServer(unpack(args))
		index = index + 1
		if index > #queue then index = 1 end
		task.wait(0.5)
	end
end

